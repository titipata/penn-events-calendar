{
  "name": "penn-events-calendar",
  "private": true,
  "description": "A web application that collects all events happening at the University of Pennsylvania with built-in search and recommendation engine.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "hug-dev": "cd ../backend && hug -f hug_api.py -p 8888",
    "gatsby-prod": "gatsby build && gatsby serve",
    "gatsby-prod-prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "elasticsearch": "cd ../backend && ./serve_elasticsearch.sh",
    "index-elastic": "cd ../backend && python index_elasticsearch.py",
    "start": "concurrently -n 'hug,gatsby,elasticsearch' -c 'bgBlue,bgMagenta,bgGreen' 'npm run hug-dev' 'gatsby develop' 'npm run elasticsearch'",
    "deploy": "concurrently -n 'hug,gatsby,elasticsearch' -c 'bgBlue,bgMagenta,bgGreen' 'npm run hug-dev' 'npm run gatsby-prod' 'npm run elasticsearch'",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@reach/router": "^1.2.1",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.4.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-csv": "^2.0.8",
    "gatsby-transformer-json": "^2.2.9",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-pagination": "^1.20.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-spinners": "^0.6.1",
    "react-typography": "^0.16.19",
    "shortid": "^2.2.14",
    "styled-components": "^4.2.0",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19",
    "use-global-hook": "^0.1.10"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.15",
    "concurrently": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "gatsby-plugin-eslint": "^2.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/titipata/penn-events-calendar"
  },
  "bugs": {
    "url": "https://github.com/titipata/penn-events-calendar/issues"
  }
}
